<h1>Quiz Results</h1>
<p id="question">You answerd correctly <%= @correct %> out of <%= @total %> questions.</p>
<p id="scorre"><%= @score %> %</p>
<%= form_tag({:controller => 'quiz', :action => 'index'}, :method => 'get') do %>
<div id="center">
  <%= submit_tag('Take another quiz', :class => 'submit') %>
</div>
<% end %>
<% content_for(:title) do %>
Quiz Results
<% end %>
